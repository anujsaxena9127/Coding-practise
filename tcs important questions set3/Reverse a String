
<a href="https://practice.geeksforgeeks.org/problems/reverse-the-string/0">Visit</a>
class GFG {
	public static void main (String[] args) {
	
	Scanner sc= new Scanner(System.in);
	int t=sc.nextInt();
	while(t-->0)
	{
	    String s=sc.next();
	    
	    
	    for(int i=s.length()-1;i>=0;i--)
	    {
	        System.out.print(s.charAt(i));
	    }
	    System.out.println();
	    
	}
	}
}
